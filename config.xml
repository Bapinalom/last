<?xml version="1.0" encoding="UTF-8" ?>
<widget id="com.rechk.game" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>rechk</name>
	<description>newad</description>
	<author href="bapin.com" email="bapin.sk@gmail.com">bapin</author>

	<icon src="www/icon-256.png" width="256" height="256" density="xxxhdpi" />
	<icon src="www/icon-128.png" width="128" height="128" density="xhdpi" />

	<platform name="android">
		<splash src="www/icons/default@2x~universal~anyany.png" />
		<preference name="android-minSdkVersion" value="21" />
		<preference name="android-targetSdkVersion" value="26" />
	</platform>
	
	<plugin name="cocoon-plugin-ads-android-admob" />
	<plugin name="cordova-plugin-whitelist" />
	
	<preference name="orientation" value="landscape" />
	<preference name="fullscreen" value="false" />
	<preference name="DisallowOverscroll" value="true" />
	<preference name="LoadUrlTimeoutValue" value="120000" />
	<access origin="*" />

	<engine name="android" />

</widget>
